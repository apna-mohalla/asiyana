{
  "name": "frontend",
  "version": "1.0.0",
  "description": "This is a small try to automate things in our apartment.",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "main": "index.js",
  "scripts": {
    "deploy": "NODE_ENV=production webpack --mode production --config webpack.config.js && npm run publish-site",
    "lint": "eslint .eslintrc.json src/js/",
    "lint:fix": "eslint .eslintrc.json src/js/ --fix",
    "sass-lint": "sass-lint -c ./.sass-lint.yml -v -q",
    "sass-lint:fix": "sass-lint-auto-fix",
    "serve": "webpack-dev-server --open",
    "publish-site": "bash deploy.sh",
    "start": "webpack --mode development --config webpack.config.js",
    "validate": "npm ls"
  },
  "pre-commit": [
    "sass-lint",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apna-mohalla/frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apna-mohalla/frontend/issues"
  },
  "homepage": "https://github.com/apna-mohalla/frontend#readme",
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "precommit-hook": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.17.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
